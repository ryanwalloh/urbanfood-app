{
  "expo": {
    "name": "Soti Rider",
    "slug": "rider-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sotirider",
    "plugins": [
      [
        "expo-dev-client",
        {
          "scheme": "sotirider"
        }
      ]
    ],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F43332"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCCuDLJMhB-23kQiXYpXwi-yYGvKz7OgSQ"
        }
      },
      "package": "com.ryamazingw.sotirider"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fe14a901-9b7c-4794-9108-3377d61f66ef"
      }
    },
    "owner": "ryanwalloh"
  }
}
